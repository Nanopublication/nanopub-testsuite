  sub:step a bpmn:ScriptTask, pplan:Step;
    ns1:description """@mark_as_fairstep(label='add integers', is_script_task=True)
def add(a: int, b: int) -> int:
    \"\"\"
    Computational step adding two ints together.
    \"\"\"
    return a + b
""";
